package selfProgramming;
import java.util.Scanner;
public class multipleSelectionScore {

		public static void main(String[] args) {
			// TODO Auto-generated method stub
			//if nested is if statement can be inside another if statement 
			Scanner input = new Scanner (System.in);
			
			System.out.println("Enter a Score :");
			int score = input.nextInt();
			
			if (score>=90)
				System.out.print("A");
			else if (score>=80)
				System.out.print("B");
			else if (score>=70)
				System.out.print("C");
			else if (score>=60)
				System.out.print("D");
			else 
				System.out.print("F");
			
			
		}			
}
